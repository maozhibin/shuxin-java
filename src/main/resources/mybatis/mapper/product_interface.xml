<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.baoquan.shuxin.dao.ProductInterfaceDao">
    <select id="queryByReflectMethod" resultType="com.baoquan.shuxin.model.ProductInterface">
        SELECT
            id,
            product_id,
            free,
            reflect_method
        FROM product_interface
        WHERE reflect_method = #{reflectMethod}
        LIMIT 1
    </select>
</mapper>