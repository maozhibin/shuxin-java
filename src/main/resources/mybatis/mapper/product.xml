<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.baoquan.shuxin.dao.ProductDao">
    <select id="queryByReflectClass" resultType="com.baoquan.shuxin.model.Product">
        select id,reflect_class
        from product
        where reflect_class = #{reflectClass}
    </select>
</mapper>